<span class="p">[</span><span class="n">plex</span><span class="p">]</span><span class="w"> </span><span class="p">[</span><span class="mi">3</span><span class="o">/</span><span class="mi">3</span><span class="p">]</span><span class="w"> </span><span class="n">Installing</span><span class="w"> </span><span class="n">plexmediaserver</span><span class="o">-</span><span class="mf">1.13</span><span class="o">.</span><span class="mf">8.5395</span><span class="o">...</span><span class="w"></span>
<span class="o">===&gt;</span><span class="w"> </span><span class="n">Creating</span><span class="w"> </span><span class="n">groups</span><span class="o">.</span><span class="w"></span>
<span class="n">Creating</span><span class="w"> </span><span class="n">group</span><span class="w"> </span><span class="s1">&#39;plex&#39;</span><span class="w"> </span><span class="n">with</span><span class="w"> </span><span class="n">gid</span><span class="w"> </span><span class="s1">&#39;972&#39;</span><span class="o">.</span><span class="w"></span>
<span class="o">===&gt;</span><span class="w"> </span><span class="n">Creating</span><span class="w"> </span><span class="n">users</span><span class="w"></span>
